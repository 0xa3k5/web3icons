{
  "name": "@web3icons/core",
  "homepage": "https://tokenicons.io",
  "description": "Token Icons' core library serving optimized SVGs for cryptocurrency token & coin icons.",
  "keywords": [
    "crypto icons",
    "cryptocurrency",
    "ethereum logos",
    "cryptocurrency logos",
    "crypto logos",
    "svg crypto icons",
    "coin icons"
  ],
  "version": "3.0.1",
  "private": false,
  "author": "0xa3k5",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "./dist/svgs/tokens/*",
    "./dist/svgs/networks/*",
    "./dist/metadata/index.js"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./metadata": "./dist/metadata/index.js",
    "./svgs/tokens/*": "./dist/svgs/tokens/*",
    "./svgs/networks/*": "./dist/svgs/networks/*"
  },
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/0xa3k5/web3icons.git",
    "directory": "packages/core"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "(cd ../utils && bun run pre-build:core) && tsc && bun run lint && bun run format && bun run post-build",
    "post-build": "bun run src/post-build-fix-paths.ts",
    "lint": "eslint ./src --ext .ts",
    "format": "prettier --write \"**/*.{ts,js,md,json}\" --log-level error"
  },
  "dependencies": {
    "@web3icons/utils": "0.0.10"
  },
  "devDependencies": {
    "bun-types": "latest",
    "cheerio": "^1.0.0-rc.12",
    "svgo": "^3.2.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}
