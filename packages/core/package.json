{
  "name": "@web3icons/core",
  "homepage": "https://web3icons.io",
  "description": "Token Icons' core library serving optimized SVGs for cryptocurrency token & coin icons.",
  "keywords": [
    "crypto icons",
    "cryptocurrency",
    "ethereum logos",
    "cryptocurrency logos",
    "crypto logos",
    "svg crypto icons",
    "coin icons"
  ],
  "version": "4.0.43",
  "private": false,
  "author": "0xa3k5",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./svgs/tokens/*": "./dist/svgs/tokens/*",
    "./svgs/networks/*": "./dist/svgs/networks/*",
    "./svgs/wallets/*": "./dist/svgs/wallets/*"
  },
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/0xa3k5/web3icons.git",
    "directory": "packages/core"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "bun run pre-build && rollup -c && bun format",
    "pre-build": "cd ../../scripts/build-scripts/core && bun core.pre-build.ts",
    "lint": "eslint ./src --ext .ts",
    "format": "prettier --write \"**/*.{ts,js,md,json}\" --log-level error"
  },
  "dependencies": {
    "@web3icons/common": "0.11.38"
  },
  "devDependencies": {
    "bun-types": "latest",
    "cheerio": "^1.0.0-rc.12",
    "svgo": "^3.2.0",
    "rollup-plugin-string": "^3.0.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}
