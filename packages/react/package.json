{
  "name": "@web3icons/react",
  "homepage": "https://web3icons.io",
  "description": "React components for incorporating Web 3 Icons into React applications.",
  "keywords": [
    "crypto icons",
    "crypto icons react",
    "cryptocurrency react",
    "react blockchain icons",
    "icon components",
    "crypto logos",
    "coin icons"
  ],
  "version": "4.1.17",
  "private": false,
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./dynamic": {
      "types": "./dist/dynamic/index.d.ts",
      "import": "./dist/dynamic/index.js"
    },
    "./dynamicIconImports": {
      "types": "./dynamicIconImports.d.ts",
      "import": "./dynamicIconImports.js"
    },
    "./icons/tokens/*": {
      "types": "./dist/icons/tokens/*.d.ts",
      "import": "./dist/icons/tokens/*.js"
    },
    "./icons/networks/*": {
      "types": "./dist/icons/networks/*.d.ts",
      "import": "./dist/icons/networks/*.js"
    },
    "./icons/wallets/*": {
      "types": "./dist/icons/wallets/*.d.ts",
      "import": "./dist/icons/wallets/*.js"
    },
    "./icons/exchanges/*": {
      "types": "./dist/icons/exchanges/*.d.ts",
      "import": "./dist/icons/exchanges/*.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "bun clean && (cd ../../scripts/build-scripts/react && bun react.pre-build.ts) && rollup -c && rollup -c rollup.dynamic.config.mjs && bun format",
    "dev": "bun run build -- --watch",
    "lint": "eslint ./src --ext .ts,.tsx",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,md,mdx,json}\" --log-level error",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@web3icons/common": "0.11.46"
  },
  "devDependencies": {
    "@svgr/core": "^8.1.0",
    "@types/react": "^18.3.3",
    "bun-types": "latest",
    "react": "^18.2.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0"
  },
  "files": [
    "dist",
    "dynamicIconImports.js",
    "dynamicIconImports.js.map",
    "dynamicIconImports.d.ts"
  ],
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/0xa3k5/web3icons.git",
    "directory": "packages/react"
  }
}
